{
  "name": "conjure-lite",
  "version": "0.4.1",
  "license": "Apache-2.0",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "conjure-lite": "./dist/conjure-lite.mjs"
  },
  "files": [
    "package.json",
    "dist"
  ],
  "scripts": {
    "prepack": "pnpm run clean && pnpm run build && pnpm run generateExamples && pnpm run format",
    "clean": "rm -rf lib tsconfig.tsbuildinfo dist",
    "build": "pkgroll --target=es2020",
    "format": "dprint fmt",
    "generateExamples": "rm -rf examples/widget/src/generated examples/widget/lib examples/widget/tsconfig.tsbuildinfo && ./dist/conjure-lite.mjs generate --ir examples/widget/ir.json --outDir examples/widget/src/generated && tsc --build examples/widget",
    "check": "pnpm run prepack && attw --pack"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.38.4",
    "@types/node": "^20.9.0",
    "@types/yargs": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "conjure-api": "^4.31.0",
    "dprint": "^0.42.5",
    "eslint": "^8.57.0",
    "tsup": "^8.0.2",
    "typescript": "^5.2.2",
    "@changesets/cli": "^2.27.1",
    "@arethetypeswrong/cli": "^0.15.2",
    "pkgroll": "^2.0.2"
  },
  "dependencies": {
    "dedent": "^1.5.1",
    "yargs": "^17.7.2"
  }
}
